(executable
 (name discover)
 (libraries core stdio)
 (preprocess (pps ppx_let))
 (modules discover))

(rule
 (deps (file discover.exe))
 (targets config.ml)
 (action (with-stdout-to config.ml
          (run ./discover.exe %{project_root} %{workspace_root}))))

(ignored_subdirs (bench))

